{
  "name": "sql",
  "rules": {
    "program": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "statement"
        }
      ]
    },
    "keyword_select": {
      "type": "PATTERN",
      "value": "select|SELECT"
    },
    "keyword_from": {
      "type": "PATTERN",
      "value": "from|FROM"
    },
    "keyword_where": {
      "type": "PATTERN",
      "value": "where|WHERE"
    },
    "comment": {
      "type": "PATTERN",
      "value": "--.*\\n"
    },
    "statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_select_statement"
        }
      ]
    },
    "_select_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "select"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "from"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "select": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword_select"
        },
        {
          "type": "SYMBOL",
          "name": "select_expression"
        }
      ]
    },
    "select_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "*"
        },
        {
          "type": "SYMBOL",
          "name": "_field_list"
        }
      ]
    },
    "_field_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "field"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "field"
              }
            ]
          }
        }
      ]
    },
    "field": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "FIELD",
                      "name": "table_alias",
                      "content": {
                        "type": "SYMBOL",
                        "name": "identifier"
                      }
                    },
                    {
                      "type": "STRING",
                      "value": "."
                    }
                  ]
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "FIELD",
              "name": "name",
              "content": {
                "type": "SYMBOL",
                "name": "identifier"
              }
            }
          ]
        },
        {
          "type": "PREC",
          "value": 2,
          "content": {
            "type": "SYMBOL",
            "name": "_number"
          }
        }
      ]
    },
    "from": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword_from"
        },
        {
          "type": "SYMBOL",
          "name": "table_expression"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "where"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "table_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "table_alias",
              "content": {
                "type": "SYMBOL",
                "name": "identifier"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "where": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword_where"
        },
        {
          "type": "SYMBOL",
          "name": "where_expression"
        }
      ]
    },
    "where_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "predicate"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "AND"
              },
              {
                "type": "SYMBOL",
                "name": "predicate"
              }
            ]
          }
        }
      ]
    },
    "predicate": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "field"
        },
        {
          "type": "SYMBOL",
          "name": "operator"
        },
        {
          "type": "SYMBOL",
          "name": "literal"
        }
      ]
    },
    "operator": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "="
        }
      ]
    },
    "literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_number"
        },
        {
          "type": "SYMBOL",
          "name": "_string"
        }
      ]
    },
    "identifier": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_string"
        },
        {
          "type": "SYMBOL",
          "name": "_number"
        }
      ]
    },
    "_string": {
      "type": "PATTERN",
      "value": "([a-zA-Z_$][0-9a-zA-Z_]*)"
    },
    "_number": {
      "type": "PATTERN",
      "value": "\\d+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s\\n"
    },
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

