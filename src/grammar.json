{
  "name": "sql",
  "rules": {
    "program": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "statement"
        }
      ]
    },
    "keyword_select": {
      "type": "PATTERN",
      "value": "select|SELECT"
    },
    "keyword_from": {
      "type": "PATTERN",
      "value": "from|FROM"
    },
    "keyword_where": {
      "type": "PATTERN",
      "value": "where|WHERE"
    },
    "keyword_order_by": {
      "type": "PATTERN",
      "value": "order by|ORDER BY"
    },
    "keyword_group_by": {
      "type": "PATTERN",
      "value": "group by|GROUP BY"
    },
    "keyword_having": {
      "type": "PATTERN",
      "value": "having|HAVING"
    },
    "keyword_desc": {
      "type": "PATTERN",
      "value": "desc|DESC"
    },
    "keyword_asc": {
      "type": "PATTERN",
      "value": "asc|ASC"
    },
    "keyword_limit": {
      "type": "PATTERN",
      "value": "limit|LIMIT"
    },
    "keyword_offset": {
      "type": "PATTERN",
      "value": "offset|OFFSET"
    },
    "keyword_distinct": {
      "type": "PATTERN",
      "value": "distinct|DISTINCT"
    },
    "keyword_count": {
      "type": "PATTERN",
      "value": "count|COUNT"
    },
    "keyword_max": {
      "type": "PATTERN",
      "value": "max|MAX"
    },
    "keyword_min": {
      "type": "PATTERN",
      "value": "min|MIN"
    },
    "keyword_avg": {
      "type": "PATTERN",
      "value": "avg|AVG"
    },
    "comment": {
      "type": "PATTERN",
      "value": "--.*\\n"
    },
    "marginalia": {
      "type": "PATTERN",
      "value": "\\/'*.*\\*\\/"
    },
    "statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_select_statement"
        }
      ]
    },
    "_select_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "select"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "from"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "select": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword_select"
        },
        {
          "type": "SYMBOL",
          "name": "select_expression"
        }
      ]
    },
    "select_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_field_list"
        }
      ]
    },
    "_field_list": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "*"
        },
        {
          "type": "SYMBOL",
          "name": "literal"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "function_call"
                },
                {
                  "type": "SYMBOL",
                  "name": "field"
                }
              ]
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": ","
                  },
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "SYMBOL",
                        "name": "function_call"
                      },
                      {
                        "type": "SYMBOL",
                        "name": "field"
                      }
                    ]
                  }
                ]
              }
            }
          ]
        }
      ]
    },
    "field": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC",
          "value": 1,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "FIELD",
                        "name": "table_alias",
                        "content": {
                          "type": "SYMBOL",
                          "name": "identifier"
                        }
                      },
                      {
                        "type": "STRING",
                        "value": "."
                      }
                    ]
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              },
              {
                "type": "FIELD",
                "name": "name",
                "content": {
                  "type": "SYMBOL",
                  "name": "identifier"
                }
              }
            ]
          }
        },
        {
          "type": "PREC",
          "value": 2,
          "content": {
            "type": "SYMBOL",
            "name": "_number"
          }
        }
      ]
    },
    "function_call": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "function_name",
          "content": {
            "type": "SYMBOL",
            "name": "_function_name"
          }
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "FIELD",
          "name": "parameter",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "function_call"
              },
              {
                "type": "FIELD",
                "name": "parameter",
                "content": {
                  "type": "SYMBOL",
                  "name": "field"
                }
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "_function_name": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword_distinct"
        },
        {
          "type": "SYMBOL",
          "name": "keyword_count"
        },
        {
          "type": "SYMBOL",
          "name": "keyword_max"
        },
        {
          "type": "SYMBOL",
          "name": "keyword_min"
        },
        {
          "type": "SYMBOL",
          "name": "keyword_avg"
        }
      ]
    },
    "from": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword_from"
        },
        {
          "type": "SYMBOL",
          "name": "table_expression"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "where"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "group_by"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "order_by"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "limit"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "table_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "table_alias",
              "content": {
                "type": "SYMBOL",
                "name": "identifier"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "where": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword_where"
        },
        {
          "type": "SYMBOL",
          "name": "where_expression"
        }
      ]
    },
    "group_by": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword_group_by"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_having"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_having": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword_having"
        },
        {
          "type": "SYMBOL",
          "name": "predicate"
        }
      ]
    },
    "order_by": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword_order_by"
        },
        {
          "type": "SYMBOL",
          "name": "field"
        },
        {
          "type": "SYMBOL",
          "name": "direction"
        }
      ]
    },
    "limit": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword_limit"
        },
        {
          "type": "SYMBOL",
          "name": "literal"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "offset"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "offset": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword_offset"
        },
        {
          "type": "SYMBOL",
          "name": "literal"
        }
      ]
    },
    "where_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "predicate"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "AND"
              },
              {
                "type": "SYMBOL",
                "name": "predicate"
              }
            ]
          }
        }
      ]
    },
    "predicate": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "field"
        },
        {
          "type": "SYMBOL",
          "name": "operator"
        },
        {
          "type": "SYMBOL",
          "name": "literal"
        }
      ]
    },
    "operator": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "STRING",
          "value": ">"
        },
        {
          "type": "STRING",
          "value": "<"
        }
      ]
    },
    "direction": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword_desc"
        },
        {
          "type": "SYMBOL",
          "name": "keyword_asc"
        }
      ]
    },
    "literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_number"
        },
        {
          "type": "SYMBOL",
          "name": "_literal_string"
        }
      ]
    },
    "identifier": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_string"
        },
        {
          "type": "SYMBOL",
          "name": "_number"
        }
      ]
    },
    "_literal_string": {
      "type": "PATTERN",
      "value": "(['\"])([a-zA-Z_$][0-9a-zA-Z_]*['\"])"
    },
    "_string": {
      "type": "PATTERN",
      "value": "([a-zA-Z_$][0-9a-zA-Z_]*)"
    },
    "_number": {
      "type": "PATTERN",
      "value": "\\d+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s\\n"
    },
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "SYMBOL",
      "name": "marginalia"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

